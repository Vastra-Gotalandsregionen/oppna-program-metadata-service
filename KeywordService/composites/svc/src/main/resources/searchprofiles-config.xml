<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <!-- ========================================================================== -->
    <!-- == Defines the search profiles used in services-config.xml              == -->
    <!-- ========================================================================== -->
 
    <bean id="searchProfile.vgr" class="se.vgregion.metaservice.keywordservice.domain.SearchProfile">
        <property name="profileId" value="${vgr.profileid}" />
        <property name="searchableNamespaces">
            <set>
                <value>VGR</value>
                <value>SweMESH</value>
            </set>
        </property>
        <property name="writeableNamespaces">
            <set>
                <value>VGR</value>
                <value>vgr</value>
            </set>
        </property>
    </bean>
    <bean id="searchProfile.vgr_epi" class="se.vgregion.metaservice.keywordservice.domain.SearchProfile">
        <property name="profileId" value="${vgr_epi.profileid}" />
        <property name="searchableNamespaces">
            <set>
                <value>VGR</value>
                <value>SweMESH</value>
            </set>
        </property>
        <property name="writeableNamespaces">
            <set>
                <value>VGR</value>
            </set>
        </property>
    </bean>
    <bean id="searchProfile.vgr_semsys" class="se.vgregion.metaservice.keywordservice.domain.SearchProfile">
        <property name="profileId" value="${vgr_semsys.profileid}" />
        <property name="searchableNamespaces">
            <set>
                <value>MeSH R</value>
                <value>SweMESH</value>
            </set>
        </property>
        <property name="writeableNamespaces">
            <set>
                <value></value>
            </set>
        </property>
    </bean>

    <bean id="searchProfile.vgr_barium" class="se.vgregion.metaservice.keywordservice.domain.SearchProfile">
        <property name="profileId" value="${vgr_barium.profileid}" />
        <property name="searchableNamespaces">
            <set>
                <value>VGR</value>
                <value>Regionarkivet</value>
                <value>SweMeSH</value>
                <value>Dokumenttyp VGR</value>
                <value>Handlingstyp</value>
                <value>Språk</value>
                <value>Verksamhetskod</value>
                <value>Dokumentstatus</value>
                <value>HosPersKat</value>
                <value>Legitimerade yrken</value>
                <value>Specialistutbildningar</value>
            </set>
        </property>
        <property name="writeableNamespaces">
            <set>
                <value></value>
            </set>
        </property>
    </bean>
    	
    <bean id="searchProfile.vgr_alfresco" class="se.vgregion.metaservice.keywordservice.domain.SearchProfile">
        <property name="profileId" value="${vgr_alfresco.profileid}" />
        <property name="searchableNamespaces">
            <set>
                <value>VGR</value>
                <value>Regionarkivet</value>
                <value>SweMeSH</value>
                <value>Dokumenttyp VGR</value>
                <value>Handlingstyp</value>
                <value>Språk</value>
                <value>Verksamhetskod</value>
                <value>Dokumentstatus</value>
                <value>HosPersKat</value>
                <value>Legitimerade yrken</value>
                <value>Specialistutbildningar</value>
            </set>
        </property>
        <property name="writeableNamespaces">
            <set>
                <value></value>
            </set>
        </property>
    </bean>

</beans>
